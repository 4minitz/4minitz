{
  "name": "4Minitz",
  "version": "v2.0.0-develop",
  "4minitz": {
    "//": "This is some app specific info",
    "4m_branch": "develop",
    "4m_commitlong": "1202f41f7a713ad408816f71cd97e5440cd59bbc",
    "4m_commitshort": "1202f41f",
    "4m_releasedate": "2020-07-27"
  },
  "description": "Simply the best free webapp for taking meeting minutes.  ;-)",
  "main": "4minitz.html",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint client/** imports/** server/**",
    "eslint:fix": "eslint --fix client/** imports/** server/**",
    "mdlint": "markdownlint -- ./*.md ./doc/**/*.md || true",
    "test": "npm run test:unit",
    "test:unit": "npx mocha \"./tests/unit/**/*.test.js\" --require @babel/register --require tests/unit/babel && npm run test:fixtures:seed-minutes:unit",
    "test:i18n": "node tests/i18n/test_i18n_resources.js",
    "test:ci": "npm run test:unit && npm run test:i18n && npm run test:end2end:headless",
    "test:end2end:ldap": "node tests/support/ldap-server.js                 # used by test:end2end:server",
    "test:end2end:meteor": "meteor run --port 3100 --settings ./settings-test-end2end.json    # used by test:end2end:server",
    "test:end2end:server": "node tests/support/end2end-server.js            # MAIN! Use this to launch an end2end server!",
    "@comment test:end2end": "**HINT** All end2end script execute all tests, except you append a -- --spec=tests/end2end/MyTest-test.js",
    "test:end2end": "unset HEADLESS && export NODE_ENV=end2end && npx wdio run wdio.conf.js",
    "test:end2end:headless": "export HEADLESS=1 && export NODE_ENV=end2end && npx wdio run wdio.conf.js",
    "test:end2end:watch": "unset HEADLESS && export NODE_ENV=end2end && npx wdio run wdio.conf.js --watch",
    "test:end2end:watchheadless": "export HEADLESS=1 && export NODE_ENV=end2end && npx wdio run wdio.conf.js --watch",
    "test:fixtures:seed-minutes:unit": "npx mocha  --recursive tests/performance/fixtures/tests/ --require @babel/register --require tests/performance/fixtures/tests/babel",
    "test:program:priority-analyzer": "npx mocha  --recursive programs/priority-analyzer/tests/ --require @babel/register --require programs/priority-analyzer/tests/babel",
    "progam:priority-analyzer": "node ./node_modules/babel-cli/bin/babel-node.js --presets=env programs/priority-analyzer/priority-analyzer.js",
    "fixtures:seed-minutes": "node ./node_modules/babel-cli/bin/babel-node.js --presets=env tests/performance/fixtures/insertMeetingSeries.js",
    "postinstall": "node programs/postInstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4minitz/4minitz.git"
  },
  "author": "4Minitz Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4minitz/4minitz/issues"
  },
  "homepage": "https://www.4minitz.com",
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "arrive": "^2.4.1",
    "bcrypt": "^5.0.0",
    "bootstrap": "file:imports/bootstrap",
    "bootstrap-material-design": "file:imports/bootstrap-material-design",
    "eonasdan-bootstrap-datetimepicker": "github:eonasdan/bootstrap-datetimepicker#4.17.47",
    "fibers": "^5.0.0",
    "fs-extra": "^10.0.0",
    "git-rev-sync": "^3.0.1",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.13.0",
    "jquery-ui-touch-punch": "^0.2.3",
    "ldapjs": "^2.3.1",
    "lodash": "^4.17.21",
    "md5-file": "^5.0.0",
    "meteor-node-stubs": "^1.1.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.4.0",
    "mongo-uri": "^0.1.2",
    "mongodb": "^4.0.0",
    "mongodb-extended-json": "^1.10.0",
    "node-cron": "^3.0.0",
    "node-getopt": "^0.3.2",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@wdio/cli": "^7.0.0",
    "@wdio/local-runner": "^7.0.0",
    "@wdio/mocha-framework": "^7.0.0",
    "@wdio/spec-reporter": "^7.0.0",
    "@wdio/sync": "^7.0.0",
    "acorn": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.1.2",
    "chromedriver": "^97.0.0",
    "clone": "^2.1.2",
    "ddp": "^0.12.1",
    "eslint": "^8.7.0",
    "faker": "^5.5.3",
    "js-yaml": "^4.0.0",
    "markdownlint-cli": "^0.30.0",
    "mocha": "^9.0.0",
    "proxyquire": "^1.8.0",
    "resolve": "1.20.0",
    "sinon": "^12.0.0",
    "sinon-chai": "^3.7.0",
    "tree-kill": "^1.2.0",
    "underscore": "^1.13.0-2",
    "wdio-chromedriver-service": "^7.0.0",
    "webdriverio": "^7.0.0",
    "webpack": "^5.0.0"
  }
}
